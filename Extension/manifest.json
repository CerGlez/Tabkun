{
  "manifest_version":2,
  "name": "Tabkun",
  "version":"1.5",
  
  "description": "Testeo de extension",
  "browser_action":{
  "default_icon": "icon.png",
  "default_popup": "popup.html"
  },
 
  "background": {
      "scripts": ["background.js"]
      
    },
  "permissions":[
  "tabs",
  "management",
  "http://localhost/*",
  "storage"
  ] 
 }
